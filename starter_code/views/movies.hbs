<div class="movie-library">
  <h2>Ironhack Cinema</h2>
  <p>Click on the movie to chck the showtimes!</p>
</div>

<div class="container grid">

  <div class="row">
    {{#each movies}}
    <div class="col-4-sm movie-item">
      <img src={{image}}>
      <p>{{title}}</p>
      <a href="/movie/{{_id}}">See more</a>
    </div>
    {{/each}}
  </div>

</div>